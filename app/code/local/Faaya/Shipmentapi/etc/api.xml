<?xml version="1.0"?>
<config>
    <api>
        <resources>
            <shipmentapi translate="title" module="shipmentapi">
                <model>shipmentapi/shipment_api</model>
                <title>Shipmentapi</title>
                <methods>
                   <createshipment translate="title" module="shipmentapi">
                        <title>Shipmentapi</title>
                   </createshipment>
                </methods>
                <faults module="shipmentapi">
                    <data_invalid>
                        <code>100</code>
                        <message>Invalid data</message>
                    </data_invalid>
                    <filters_invalid>
                        <code>101</code>
                        <message>Invalid filters specified. Details in error message.</message>
                    </filters_invalid>
                    <not_exists>
                        <code>102</code>
                        <message>shipmentapi not exists.</message>
                    </not_exists>
                    <not_deleted>
                        <code>103</code>
                        <message>shipmentapi not deleted. Details in error message.</message>
                    </not_deleted>
                </faults>
            </shipmentapi>
          </resources>
          <acl>
              <resources>
                <shipmentapi translate="title" module="shipmentapi">
                      <title>Shipmentapi</title>
                      <sort_order>5</sort_order>
                      <shipmentapi translate="title" module="shipmentapi">
                        <title>Create shipmentapi</title>
                      </shipmentapi>
                </shipmentapi>
              </resources>
        </acl>
      </api>
</config>

